<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smallB.QOS.dao.UserDao">
    <select id="register" parameterType="com.smallB.QOS.domain.UserDto" resultType="com.smallB.QOS.domain.UserDto">
        INSERT INTO users(`user_id`,`user_name`,`user_pw`,`user_ph`,`user_email`,`store_id`,`status`)
        VALUES (#{user_id},#{user_name},#{user_pw},#{user_ph},#{user_email},#{store_id},#{status})
    </select>

</mapper>